from typing import Optional


class ListNode:
    def __init__(self, val, prev=None, next = None):
        self.val = val
        self.prev = prev
        self.next = next


class MyLinkedList:
    
    def __init__(self):
        self.head = None
        self.tail = None
        self.size = 0

    def get(self, index: int) -> int:
        if index < 0 or index >= self.size:
            return -1
        
